{
    "presets": [
        "@babel/env",
        "@babel/react",
        [
            "@babel/preset-typescript",
            {
                "allExtensions": true,
                "isTSX": true
            }
        ]
    ],
    "plugins": [
        // "@babel/add-module-exports",
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ],
        "@babel/plugin-transform-modules-commonjs"
    ]
}